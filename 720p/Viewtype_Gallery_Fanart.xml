<includes>
	<include name="Viewtype_Gallery_Fanart">
		<control type="group">
			<visible>Control.IsVisible(59)</visible>
			<!-- Browser -->
			<control type="panel" id="59">
				<viewtype label="$LOCALIZE[31414] $LOCALIZE[20445]">icon</viewtype>
				<posx>38</posx>
				<posy>65</posy>
				<width>1200</width>
				<height>400</height>
				<scrolltime>200</scrolltime>
				<orientation>vertical</orientation>
				<onup>59</onup>
				<ondown>59</ondown>
				<include condition="!Skin.HasSetting(ViewLockdown)">OnLeft7000</include>
				<onright>61</onright>
				<pagecontrol>61</pagecontrol>
				<preloaditems>2</preloaditems>
				<include>Animation_OpenCloseFade</include>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<visible>!Skin.HasSetting(NoViewGalleryFanart)</visible>
				<visible>Window.IsActive(pictures) | [Window.IsActive(VideoLibrary) + [Container.Content(movies) | Container.Content(tvshows) | Container.Content(episodes)]]</visible>
				<itemlayout width="300" height="200">
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture>frames/ClassicFrame_Background.png</texture>
						<colordiffuse>FrameBackgroundDiffuse</colordiffuse>
					</control>
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<aspectratio scalediffuse="false">scale</aspectratio>
						<visible>Container.Content(episodes)</visible>
					</control>
					<!-- Picture Icon Image -->
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<aspectratio scalediffuse="false">scale</aspectratio>
						<visible>Window.IsActive(pictures) + !IsEmpty(ListItem.Icon)</visible>
					</control>
					<!-- Picture Fanart Image (use for thumb if no icon set) -->
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true" fallback="home/pictures.jpg">$INFO[ListItem.Property(Fanart_Image)]</texture>
						<aspectratio scalediffuse="false">scale</aspectratio>
						<visible>Window.IsActive(pictures) + IsEmpty(ListItem.Icon)</visible>
					</control>
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true" fallback="home/movies.jpg">$INFO[ListItem.Property(Fanart_Image)]</texture>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>Container.Content(movies) | [Container.Content(tvshows) + !Skin.HasSetting(landscape_jpg)]</visible>
					</control>
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true" fallback="MissingLandscapeJpeg.jpg">$INFO[ListItem.Path,,/landscape.jpg]</texture>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>Container.Content(tvshows) + Skin.HasSetting(landscape_jpg)</visible>
					</control>
					<control type="image">
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture>frames/ClassicFrameGlass_Horizontal.png</texture>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<colordiffuse>GlassDiffuse</colordiffuse>
					</control>
					<control type="image">
						<posx>8</posx>
						<posy>11</posy>
						<width>289</width>
						<height>179</height>
						<texture>frames/ClassicFrame_FanartSmall.png</texture>
					</control>
					<control type="image">
						<posx>240</posx>
						<posy>131</posy>
						<width>30</width>
						<height>30</height>
						<texture>$INFO[ListItem.Overlay]</texture>
						<colordiffuse>WatchedOverlay</colordiffuse>
						<visible>[Container.Content(movies) + !Skin.HasSetting(HideMovieWatchedOverlays) + stringcompare(ListItem.Overlay,OverlayWatched.png)] | [!Container.Content(movies) + !Skin.HasSetting(HideTVWatchedOverlays) + stringcompare(ListItem.Overlay,OverlayWatched.png)]</visible>
					</control>
				</itemlayout>
				<focusedlayout width="300" height="200">
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture>frames/ClassicFrame_Background.png</texture>
						<colordiffuse>FrameBackgroundDiffuse</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
						<aspectratio scalediffuse="false">scale</aspectratio>
						<visible>Container.Content(episodes) | Window.IsActive(pictures) | [Container.Content(tvshows) + !Skin.HasSetting(landscape_jpg)]</visible>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true" fallback="home/movies.jpg">$INFO[ListItem.Property(Fanart_Image)]</texture>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>Container.Content(movies) | [Container.Content(tvshows) + !Skin.HasSetting(landscape_jpg)]</visible>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture background="true" fallback="MissingLandscapeJpeg.jpg">$INFO[ListItem.Path,,/landscape.jpg]</texture>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>Container.Content(tvshows) + Skin.HasSetting(landscape_jpg)</visible>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>22</posx>
						<posy>27</posy>
						<width>260</width>
						<height>146</height>
						<texture>frames/ClassicFrameGlass_Horizontal.png</texture>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<colordiffuse>GlassDiffuse</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="100" end="0" time="400" />
						</animation>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>8</posx>
						<posy>11</posy>
						<width>289</width>
						<height>179</height>
						<texture>frames/ClassicFrame_FanartSmall.png</texture>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
					</control>
					<control type="image">
						<include>Animation_GalleryFanartFloat</include>
						<posx>240</posx>
						<posy>131</posy>
						<width>30</width>
						<height>30</height>
						<texture>$INFO[ListItem.Overlay]</texture>
						<colordiffuse>WatchedOverlay</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="163,91" start="70" end="125" time="300" tween="back" easing="out" />
							<effect type="fade" start="70" end="100" time="300" />
						</animation>
						<visible>[Container.Content(movies) + !Skin.HasSetting(HideMovieWatchedOverlays)] | [!Container.Content(movies) + !Skin.HasSetting(HideTVWatchedOverlays)]</visible>
					</control>
				</focusedlayout>
			</control>
			
			<!-- Browser label and flags -->
			<!-- Title -->
			<control type="label">
				<posx>40</posx>
				<posy>472</posy>
				<width>1200</width>
				<height>40</height>
				<align>center</align>
				<font>METF_TitleTextLarge</font>
				<textcolor>TitleText</textcolor>
				<label>$INFO[ListItem.Label]</label>
				<include>Animation_OpenCloseFade</include>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<visible>[Window.IsActive(VideoLibrary) + [Container.Content(movies) | Container.Content(tvshows) | Container.Content(episodes)]] | Window.IsActive(pictures)</visible>
			</control>
			<control type="grouplist">
				<posx>40</posx>
				<posy>505</posy>
				<width>1200</width>
				<height>30</height>
				<align>center</align>
				<itemgap>10</itemgap>
				<orientation>horizontal</orientation>
				<include>Animation_OpenCloseFade</include>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<visible>Window.IsActive(VideoLibrary) + [Container.Content(movies) | Container.Content(tvshows) | Container.Content(episodes)]</visible>
				<!-- Genre -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsGenre</textcolor>
					<label>$INFO[ListItem.Genre]</label>
					<visible>!IsEmpty(ListItem.Genre)</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>![IsEmpty(ListItem.Genre) | IsEmpty(ListItem.Year)]</visible>
				</control>
				<!-- Year -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsYear</textcolor>
					<label>$INFO[ListItem.Year]</label>
					<visible>!IsEmpty(ListItem.Year) + !Container.Content(episodes)</visible>
				</control>
				<!-- Episode First Aired Date -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsInfo</textcolor>
					<label>$LOCALIZE[20416] $INFO[ListItem.Date]</label>
					<visible>!IsEmpty(ListItem.Date) + Container.Content(episodes)</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>![IsEmpty(ListItem.Year) | IsEmpty(ListItem.Duration)] + [Container.Content(movies) | Container.Content(tvshows)] | ![IsEmpty(ListItem.Date) | IsEmpty(ListItem.Duration)] + Container.Content(episodes)</visible>
				</control>
				<!-- Duration -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsDuration</textcolor>
					<label>$INFO[ListItem.Duration,, [LOWERCASE]$LOCALIZE[31299][/LOWERCASE]]</label>
					<visible>!IsEmpty(ListItem.Duration) + !SubString(ListItem.Duration,min) + !Container.Content(tvshows)</visible>
				</control>
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsDuration</textcolor>
					<label>$INFO[ListItem.Duration]</label>
					<visible>!IsEmpty(ListItem.Duration) + SubString(ListItem.Duration,min) + !Container.Content(tvshows)</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>![IsEmpty(ListItem.Duration) | IsEmpty(ListItem.Episode)] + !Container.Content(episodes) | Container.Content(tvshows)</visible>
				</control>
				<!-- Episode Count -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsEpisodes</textcolor>
					<label>$INFO[ListItem.Episode] $LOCALIZE[20360]</label>
					<visible>!IsEmpty(ListItem.Episode) + Container.Content(tvshows)</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>!IsEmpty(ListItem.Episode) + [Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
				<!-- Unwatched Episode Count -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsEpisodes</textcolor>
					<label>$INFO[ListItem.Property(UnwatchedEpisodes)] $LOCALIZE[16101]</label>
					<visible>!IsEmpty(ListItem.Episode) + [Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
			</control>
			<!-- Movie Sets -->
			<control type="label">
				<posx>240</posx>
				<posy>503</posy>
				<width>800</width>
				<height>30</height>
				<align>center</align>
				<aligny>center</aligny>
				<font>METF_MediaDetailsSmall</font>
				<textcolor>MediaDetailsGenre</textcolor>
				<label>$LOCALIZE[31313]</label>
				<include>Animation_OpenCloseFade</include>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<visible>IsEmpty(ListItem.Genre) + Container.Content(movies) + IsEmpty(ListItem.path) + !StringCompare(ListItem.Label,..)</visible>
			</control>
			<!-- Arrows -->
			<control type="group">
				<include>Animation_OpenCloseFade</include>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<visible>Window.IsActive(VideoLibrary) + [Container.Content(movies) | Container.Content(tvshows) | Container.Content(episodes)]</visible>
				<control type="image">
					<posx>10</posx>
					<posy>508</posy>
					<width>24</width>
					<height>24</height>
					<texture>Arrow2Up.png</texture>
					<colordiffuse>ArrowDiffuse</colordiffuse>
					<visible>Container.HasPrevious</visible>
				</control>
				<control type="image">
					<posx>1246</posx>
					<posy>508</posy>
					<width>24</width>
					<height>24</height>
					<texture>Arrow2Down.png</texture>
					<colordiffuse>ArrowDiffuse</colordiffuse>
					<visible>Container.HasNext</visible>
				</control>
			</control>
			<include>ScrollBar_Vertical</include>
		</control>
	</include>
</includes>
