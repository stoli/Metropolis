<includes>
	<include name="Viewtype_Fanart">
		<control type="group">
			<visible>Control.IsVisible(504)</visible>
			<!-- Browser -->
			<control type="wraplist" id="504">
				<posx>20</posx>
				<posy>40</posy>
				<height>420</height>
				<width>1240</width>
				<include condition="!Skin.HasSetting(ViewLockdown)">OnUp7000</include>
				<ondown>60</ondown>
				<onleft>504</onleft>
				<onright>504</onright>
				<viewtype label="20445">bigwrap</viewtype>
				<orientation>horizontal</orientation>
				<focusposition>1</focusposition>
				<scrolltime>200</scrolltime>
				<preloaditems>2</preloaditems>
				<pagecontrol>60</pagecontrol>
				<include>Animation_FadedByMenu</include>
				<include>Animation_OpenCloseFade</include>
				<include>Animation_HiddenByInfo</include>
				<visible>!Skin.HasSetting(NoViewFanart)</visible>
				<visible>Container.Content(movies) | Container.Content(tvshows)</visible>
				<itemlayout width="260" height="420">
					<control type="image">
						<posx>28</posx>
						<posy>60</posy>
						<width>204</width>
						<height>300</height>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="image">
						<posx>28</posx>
						<posy>60</posy>
						<width>204</width>
						<height>300</height>
						<texture>frames/ClassicFrameGlass_Vertical.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<colordiffuse>GlassDiffuse</colordiffuse>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="image">
						<posx>8</posx>
						<posy>37</posy>
						<width>247</width>
						<height>346</height>
						<texture>frames/ClassicFrame_PosterSmall.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="image">
						<posx>191</posx>
						<posy>319</posy>
						<width>32</width>
						<height>32</height>
						<fadetime>200</fadetime>
						<texture>$INFO[ListItem.Overlay]</texture>
						<colordiffuse>WatchedOverlay</colordiffuse>
						<visible>[Container.Content(movies) + !Skin.HasSetting(HideMovieWatchedOverlays)] | [!Container.Content(movies) + !Skin.HasSetting(HideTVWatchedOverlays)]</visible>
					</control>
				</itemlayout>
				<focusedlayout width="720" height="420">
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture>frames/ClassicFrame_SegmentedDiffuse.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<colordiffuse>FrameBackgroundDiffuse</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + !Skin.HasSetting(NoSegmentedFanart)</visible>
					</control>
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture>frames/ClassicFrame_Background.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<colordiffuse>FrameBackgroundDiffuse</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + Skin.HasSetting(NoSegmentedFanart)</visible>
					</control>
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture fallback="home/movies.jpg" diffuse="frames/ClassicFrame_SegmentedDiffuse.png">$INFO[ListItem.Property(Fanart_Image)]</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + !Skin.HasSetting(NoSegmentedFanart) + ![Skin.HasSetting(landscape_jpg) + Container.Content(tvshows)]</visible>
					</control>
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture fallback="MissingLandscapeJpeg.jpg" diffuse="frames/ClassicFrame_SegmentedDiffuse.png">$INFO[ListItem.Path]landscape.jpg</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + !Skin.HasSetting(NoSegmentedFanart) + [Skin.HasSetting(landscape_jpg) + Container.Content(tvshows)]</visible>
					</control>
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture fallback="home/movies.jpg">$INFO[ListItem.Property(Fanart_Image)]</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + Skin.HasSetting(NoSegmentedFanart) + ![Skin.HasSetting(landscape_jpg) + Container.Content(tvshows)]</visible>
					</control>
					<control type="image">
						<posx>32</posx>
						<posy>31</posy>
						<width>646</width>
						<height>362</height>
						<texture fallback="MissingLandscapeJpeg.jpg">$INFO[ListItem.Path]landscape.jpg</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + Skin.HasSetting(NoSegmentedFanart) + [Skin.HasSetting(landscape_jpg) + Container.Content(tvshows)]</visible>
					</control>
					<control type="image">
						<posx>9</posx>
						<posy>10</posy>
						<width>700</width>
						<height>410</height>
						<texture>frames/ClassicFrame_Segmented.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + !Skin.HasSetting(NoSegmentedFanart)</visible>
					</control>
					<control type="image">
						<posx>9</posx>
						<posy>10</posy>
						<width>700</width>
						<height>410</height>
						<texture>frames/ClassicFrame_FanartMedium.png</texture>
						<fadetime>200</fadetime>
						<aspectratio scalediffuse="false">stretch</aspectratio>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="auto" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>!IsEmpty(ListItem.Title) + Skin.HasSetting(NoSegmentedFanart)</visible>
					</control>
					<control type="image">
						<posx>638</posx>
						<posy>352</posy>
						<width>32</width>
						<height>32</height>
						<fadetime>200</fadetime>
						<texture>$INFO[ListItem.Overlay]</texture>
						<colordiffuse>WatchedOverlay</colordiffuse>
						<animation type="Focus" reversible="false">
							<effect type="zoom" center="323,181" start="70" end="100" time="400" tween="back" easing="out" />
							<effect type="fade" start="0" end="100" time="300" />
						</animation>
						<visible>[Container.Content(movies) + !Skin.HasSetting(HideMovieWatchedOverlays)] | [!Container.Content(movies) + !Skin.HasSetting(HideTVWatchedOverlays)]</visible>
					</control>
				</focusedlayout>
			</control>
			<!-- Browser label and flags -->
			<control type="group">
				<posx>40</posx>
				<posy>443</posy>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<include>Animation_OpenCloseFade</include>
				<visible>Container.Content(movies) | Container.Content(tvshows)</visible>
				<!-- Title -->
				<control type="fadelabel">
					<posx>0</posx>
					<posy>0</posy>
					<width>1200</width>
					<height>50</height>
					<align>center</align>
					<aligny>center</aligny>
					<scrollout>false</scrollout>
					<pauseatend>600</pauseatend>
					<scrollspeed>20</scrollspeed>
					<font>METF_TitleTextLarge</font>
					<textcolor>TitleText</textcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
				<!-- Genre -->
				<control type="label">
					<posx>200</posx>
					<posy>35</posy>
					<width>800</width>
					<height>30</height>
					<align>center</align>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsGenre</textcolor>
					<label>$INFO[ListItem.Genre]</label>
					<visible>!IsEmpty(ListItem.Genre)</visible>
				</control>
				<!-- Movie Sets -->
				<control type="label">
					<posx>200</posx>
					<posy>35</posy>
					<width>800</width>
					<height>30</height>
					<align>center</align>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsGenre</textcolor>
					<label>$LOCALIZE[31313]</label>
					<visible>Container.Content(movies) + IsEmpty(ListItem.path) + !StringCompare(ListItem.Label,..)</visible>
				</control>
			</control>
			<control type="grouplist">
				<posx>40</posx>
				<posy>507</posy>
				<width>1200</width>
				<height>30</height>
				<align>center</align>
				<itemgap>10</itemgap>
				<orientation>horizontal</orientation>
				<include>Animation_FadedByMenu</include>
				<include>Animation_HiddenByInfo</include>
				<include>Animation_OpenCloseFade</include>
				<visible>Container.Content(movies) | Container.Content(tvshows)</visible>
				<!-- Year -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsYear</textcolor>
					<label>$INFO[ListItem.Year]</label>
					<visible>!IsEmpty(ListItem.Year) + ![Container.Content(seasons) | Container.Content(episodes)]</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>![IsEmpty(ListItem.Year) | IsEmpty(ListItem.Duration)]</visible>
				</control>
				<!-- Duration -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsDuration</textcolor>
					<label>$INFO[ListItem.Duration,, [LOWERCASE]$LOCALIZE[31299][/LOWERCASE]]</label>
					<visible>!IsEmpty(ListItem.Duration) + !SubString(ListItem.Duration,min) + ![Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsDuration</textcolor>
					<label>$INFO[ListItem.Duration]</label>
					<visible>!IsEmpty(ListItem.Duration) + SubString(ListItem.Duration,min) + ![Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>![IsEmpty(ListItem.Duration) | IsEmpty(ListItem.Episode)] | Container.Content(tvshows)</visible>
				</control>
				<!-- Episode Count -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsEpisodes</textcolor>
					<label>$INFO[ListItem.Episode] $LOCALIZE[20360]</label>
					<visible>!IsEmpty(ListItem.Episode) + [Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
				<!-- Bullet -->
				<control type="image">
					<width>8</width>
					<height>36</height>
					<texture>Dot.png</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>BulletDiffuse</colordiffuse>
					<visible>!IsEmpty(ListItem.Episode) + [Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
				<!-- Unwatched Episode Count -->
				<control type="label">
					<width min="25" max="1200">auto</width>
					<height>30</height>
					<aligny>center</aligny>
					<font>METF_MediaDetailsSmall</font>
					<textcolor>MediaDetailsEpisodes</textcolor>
					<label>$INFO[ListItem.Property(UnwatchedEpisodes)] $LOCALIZE[16101]</label>
					<visible>!IsEmpty(ListItem.Episode) + [Container.Content(seasons) | Container.Content(tvshows)]</visible>
				</control>
			</control>
			<include>ScrollBar_Horizontal</include>
		</control>
	</include>
</includes>
